&wifi4 {
	/* qcn9224 tgt-mem-mode=0 layout - 50MB
	 * +=========+==============+=========+
	 * |  Region | Start Offset |   Size  |
	 * +---------+--------------+---------+
	 * | HREMOTE |  0x50B00000  |   28MB  |
	 * +---------+--------------+---------+
	 * | M3 Dump |  0x52700000  |   1MB   |
	 * +---------+--------------+---------+
	 * |   ETR   |  0x52800000  |   1MB   |
	 * +---------+--------------+---------+
	 * |  Caldb  |  0x52900000  |   8MB   |
	 * +---------+--------------+---------+
	 * |Pageable |  0x53100000  |  12MB   |
	 * +==================================+
	 */
	hremote_node = <&qcn9224_pcie1>;
	base-addr = <0x50B00000>;
	m3-dump-addr = <0x52700000>;
	etr-addr = <0x52800000>;
	caldb-addr = <0x52900000>;
	pageable-addr = <0x53100000>;
	hremote-size = <0x1C00000>;
	pageable-size = <0xC00000>;
	board_id = <0x04>;
	status = "ok";
};

